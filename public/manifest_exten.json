{
  "name": "Twitch BanPicker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Twitch BanPicker like League of Legends Style with viewers",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_title": "Twitch BanPicker Open"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": ["storage", "tabs"]
}
